" always open help in new tab
set newtab+=help

set hintkeys=fusntroheaidywcjklgmv
set hlfind
colorscheme solarized-dark

map <S-J> :tabnext<CR>
map <S-K> :tabprevious<CR>

set complete=suggestion,location,file,bookmark,search
